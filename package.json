{
  "name": "bumpy-fs",
  "version": "1.0.1",
  "description": "Filebase Database for Node platforms",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OgabekYuldoshev/bumpy.git"
  },
  "bugs": {
    "url": "https://github.com/OgabekYuldoshev/bumpy/issues"
  },
  "homepage": "https://github.com/OgabekYuldoshev/bumpy",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "pnpm biome check --write ./src",
    "test": "vitest run",
    "prerelease": "pnpm build && pnpm test",
    "release": "pnpm publish"
  },
  "keywords": [
    "bumpy",
    "filebase",
    "db",
    "database",
    "json"
  ],
  "author": {
    "email": "yuldashoff1@gamil.com",
    "name": "Ogabek Yuldoshev",
    "url": "https://github.com/OgabekYuldoshev"
  },
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "^2.27.9",
    "@types/node": "^22.9.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "zod": "^3.23.8"
  },
  "dependencies": {
    "flatted": "^3.3.1",
    "sift": "^17.1.3",
    "thenby": "^1.3.4"
  }
}
